{
  "name": "drm-api",
  "version": "1.0.0",
  "description": "Deerhold Resource Manager",
  "author": "Deerhold",
  "scripts": {
    "dev": "nodemon --exec ts-node src/main.ts",
    "start": "npm run build && node ./dist/main.js",
    "build": "tsc",
    "typeorm": "ts-node -r ./fix-module-alias.js ./node_modules/.bin/typeorm",
    "make:migration": "npm run typeorm -- migration:create -n",
    "migrate": "npm run typeorm -- migration:run",
    "seed:config": "ts-node -r ./fix-module-alias.js ./node_modules/typeorm-seeding/dist/cli.js config",
    "db:seed": "ts-node -r ./fix-module-alias.js ./node_modules/typeorm-seeding/dist/cli.js seed",
    "db:seed:production": "NODE_ENV=production ts-node -r ./fix-module-alias.js ./node_modules/typeorm-seeding/dist/cli.js seed",
    "code:format": "prettier --write \"src/**/*.{ts,css,js,html}\"",
    "code:check": "prettier --check \"src/**/*.{ts,css,js,html}\"",
    "code:format:specific-file": "prettier --write ",
    "test": "jest",
    "cli": "ts-node -r ./fix-module-alias.js src/utils/cli.ts"
  },
  "dependencies": {
    "@types/glob": "^7.1.3",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mailgen": "^2.0.4",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yargs": "^16.0.3",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "bullmq": "^1.32.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "class-validator-jsonschema": "^3.0.1",
    "cors": "^2.8.5",
    "cron-decorators": "^0.1.5",
    "dotenv": "^8.6.0",
    "event-dispatch": "^0.4.1",
    "express": "^4.17.1",
    "express-validator": "^6.11.1",
    "glob": "^7.1.7",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mailgen": "^2.0.14",
    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.1",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.0.0",
    "supertest": "^6.1.3",
    "swagger-ui-express": "^4.1.6",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.34",
    "typeorm-seeding": "^1.6.1",
    "typeorm-simple-query-parser": "^1.0.7",
    "typeorm-typedi-extensions": "^0.4.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/body-parser": "^1.19.0",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.12",
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.23",
    "@types/module-alias": "^2.0.0",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.17.3",
    "@types/nodemailer": "^6.4.2",
    "@types/supertest": "^2.0.11",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "ts-jest": "^26.0.23",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.2"
  }
}
